<!DOCTYPE html>
<html lang="en">
  <head>
    <%= render 'layouts/bits/google_fonts' %>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
    <%= render 'layouts/bits/meta' %>
    <title><%= content_for?(:title) ? yield(:title) + " UTS on ISS kickstarter campaign" : "UTS on ISS kickstarter campaign" %></title>
    <%= render 'layouts/bits/open_graph' %>
    <!-- Fav Icon -->
    <link rel="shortcut icon" href="/assets/favicon.ico">
    <%= render 'layouts/bits/map' %>
    <%= render 'layouts/bits/html5_shim' %>
    <%= render 'layouts/bits/add_this_header' %>
  </head>
  <body id="page-top" class="index" cz-shortcut-listen="true">
    <%= render 'layouts/bits/browsehappy' %>
    <%= render 'layouts/navigation' %>
    <%#= render 'layouts/header' %>
    <%= render 'layouts/soon' %>
    <!-- Content -->
    <div class="container-fluid">
      <%#= render 'layouts/subscribe' %>
      <%= yield %>
    </div>
      <%= render 'pages/about' %>
      <%= render 'layouts/action' %>
      <%= render 'pages/sponsors' %>
      <%= render 'layouts/contact' %>
    <div class="container">
      <%= render 'layouts/social' %>
      <%= render 'layouts/bits/ads_footer' %>
    </div>
    <%= render 'layouts/bits/analytics' %>
    <%= render 'layouts/bits/add_this_footer' %>
  </body>
</html>