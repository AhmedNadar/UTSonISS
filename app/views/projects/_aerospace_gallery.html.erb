<style type="text/css">
  section#gallery a {
    /* color: #e75967; */
    display: block;
    width: auto;
    height: auto;
    border: 1px solid #DBDBDB;
    padding: 5px;
    margin-bottom: 15px;
    background-color: rgba(245, 245, 245, 0.16);
    text-align: center;
  }
  section#gallery a img {
    width: 100%;
    height: auto;
    left: 0;
    margin: 0 auto;
    top: 0;
    position: relative;
    padding: 0;
  }

  section#gallery a img {
    height: auto;
    max-width: 100%;
    max-width: 640px;
    -moz-transition: all 1s;
    -ms-transition: all 1s;
    -o-transition: all 1s;
    -webkit-transition: all 1s;
    transition: all 1s;
  }
  section#gallery a img.expanded {
    max-width: none;
    max-width: 2000px;
  }
</style>


