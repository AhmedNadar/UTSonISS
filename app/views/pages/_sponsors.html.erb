            <div class="caption">
              <h4>Galaxy Toppers</h4>
            </div>
          <% end %>
        </div>

        <div class="col-xs-12 col-sm-6">
          <%= link_to "http://Ontario.ca" do %>
            <%= image_tag "supporters/ontario_logo_blk.png", alt: "Ontario", title: "Ontario",
