      </div><!-- /col-lg-4 -->
    </div><!-- /row -->
    <div class="row">
      <div class="centered col-sm-12 col-xs-12">
        <div class="col-xs-12 col-sm-6">
          <%= link_to "http://www.css.ca/" do %>
            <%= image_tag "supporters/canadian_space_society.png", alt: "Canadian Space Society", title: "Canadian Space Society",
            :style=>'width:150px;height:154px;margin-top:-72px;margin-left:-75px;' %>
            <div class="caption">
              <h4>Canadian Space Society</h4>
            </div>
          <% end %>
        </div>
        <div class="col-xs-12 col-sm-6">
          <%= link_to "http://www.galaxytoppers.com/" do %>
